import { IsNotEmpty, IsOptional, Matches } from 'class-validator';
import { ApiProperty } from '@nestjs/swagger';

export class Create<%= entityNameTitleCase %>Body {
    <% properties.forEach(prop => { -%>
        @ApiProperty({ required: <%= prop.required ? true : false %> }) <%= prop.required ? '@IsNotEmpty()' : '@IsOptional()' %>
        <%= prop.prop %>: <%= prop.typeTransform ? prop.typeTransform : prop.type %>;

      <% }) -%>
}
